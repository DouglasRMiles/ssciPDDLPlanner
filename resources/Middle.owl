    <!-- http://ontologies.cf.ericsson.net/Middle#Action -->

    <owl:Class rdf:about="http://ontologies.cf.ericsson.net/Middle#Action">
        <rdfs:subClassOf rdf:resource="http://ontologies.cf.ericsson.net/System#Procedural_Knowledge"/>
        <rdfs:comment> An action is part of procedural knowledge, and determines how to achieve a certain state (effect) given that an agent observes a particular state (condition) as being true in the world. </rdfs:comment>
    </owl:Class>
    


    <!-- http://ontologies.cf.ericsson.net/Middle#ActionSet -->

    <owl:Class rdf:about="http://ontologies.cf.ericsson.net/Middle#ActionSet">
        <rdfs:subClassOf rdf:resource="http://ontologies.cf.ericsson.net/System#Procedural_Knowledge"/>
        <rdfs:comment>Contains one or more (unordered) actions</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontologies.cf.ericsson.net/Middle#Actuator -->

    <owl:Class rdf:about="http://ontologies.cf.ericsson.net/Middle#Actuator">
        <rdfs:subClassOf rdf:resource="http://ontologies.cf.ericsson.net/Middle#Device"/>
        <rdfs:comment> Actuators are devices in cyber-physical agents which are responsible for allowing the given agent to affect the world. </rdfs:comment>
    </owl:Class>
    


    <!-- http://ontologies.cf.ericsson.net/Middle#Add -->

    <owl:Class rdf:about="http://ontologies.cf.ericsson.net/Middle#Add">
